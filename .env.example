# Docker Hub 配置
DOCKERHUB_USERNAME=your-username

# 版本配置
VERSION=v0.1.0-2.25.0

# 文件服务器配置
FILES_PORT=8080

# 镜像仓库配置
REGISTRY_PORT=5000
REGISTRY_HOST=hub.kubespray.local

# TLS 证书路径（如果使用 HTTPS）
TLS_CERT=/certs/hub.kubespray.local.crt
TLS_KEY=/certs/hub.kubespray.local.key

# 日志级别 (debug, info, warn, error)
LOG_LEVEL=info

# 证书目录
CERT_DIR=/opt/registry/certs
